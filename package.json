{
  "name": "@octocloud/backend",
  "type": "module",
  "version": "1.0.24",
  "license": "ISC",
  "author": "",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:dry": "tsc --noEmit",
    "build:dry:ci": "tsc --noEmit",
    "check": "npm run build:dry && npm run format && npm run lint",
    "fix": "biome check --write .",
    "format": "biome format",
    "format:fix": "biome format --write .",
    "lint": "biome lint",
    "lint:fix": "biome lint --write .",
    "test": "vitest run --config ./vitest.config.ts",
    "test:coverage": "vitest run --config ./vitest.config.ts --coverage",
    "dev": "npm run build -- --watch src",
    "lefthook": "lefthook install",
    "audit": "audit-ci --config ./audit-ci.json"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@needle-di/core": "0.10.1",
    "@octocloud/core": "^1.0.57",
    "@octocloud/types": "^1.5.24",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dinero.js": "^2.0.0-alpha.8",
    "query-string": "^8.1.0",
    "typescript": "5.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@octocloud/generators": "^1.0.6",
    "@types/node": "^22.9.0",
    "typescript": "^5.7.2",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-v8": "2.1.3",
    "audit-ci": "7.1.0",
    "lefthook": "1.10.10",
    "tsup": "8.3.5",
    "vite": "5.4.10",
    "vitest": "2.1.3",
    "vitest-mock-extended": "2.0.2"
  }
}
