{
  "name": "@octocloud/backend",
  "version": "1.0.17",
  "license": "ISC",
  "author": "",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:dry": "tsc --noEmit",
    "check": "npm run build:dry && npm run format:check && npm run lint",
    "checks": "npm run format && npm run lint && npm run test",
    "fix": "npm run format:package && npm run format && npm run lint:fix",
    "fix:staged": "npx lint-staged",
    "format": "prettier . '!./ops' --write",
    "format:check": "prettier . '!./ops' --check",
    "format:package": "prettier-package-json --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build && husky install",
    "test": "vitest run --config ./vitest.config.ts",
    "test:coverage": "vitest run --config ./vitest.config.ts --coverage",
    "watch": "npm run build -- --watch src"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@octocloud/core": "^1.0.49",
    "@octocloud/types": "^1.5.24",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dinero.js": "^2.0.0-alpha.8",
    "query-string": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "4.8.0",
    "typescript": "5.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitest/coverage-v8": "0.34.6",
    "esbuild": "^0.15.12",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "prettier-package-json": "2.8.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.3.0",
    "unbuild": "^0.9.4",
    "vite": "4.5.2",
    "vitest": "0.34.6",
    "vitest-mock-extended": "1.3.1"
  },
  "lint-staged": {
    "./package.json": "npm run format:package",
    "*": "npm run format",
    "*.(ts|js)": [
      "npm run lint:fix"
    ]
  }
}
